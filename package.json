{
  "name": "example_video_portal_vuejs",
  "version": "2.0.0",
  "description": "Example video portal site for your BoxCast account written in vue.js",
  "author": "Mike Griffith <bikegriffith@gmail.com>",
  "scripts": {
    "start": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build:dist": "vue-cli-service build",
    "build:docs": "VUE_PUBLIC_PATH=/example_video_portal_vuejs vue-cli-service build --dest=docs"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.1.0",
    "core-js": "^3.3.2",
    "jquery": "^3.5.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-unused-vars": "off",
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
